#include <stdio.h>
int main()
{
    char a;

    printf("Enter any character = ");
    scanf("%c", &a);


    if(a >= 'A' && a <= 'Z')
    {
        printf("'%c' is an uppercase alphabet.", a);
    }
    else if(a >= 'a' && a <= 'z')
    {
        printf("'%c' is a lowercase alphabet.", a);
    }
    else
    {
        printf("'%c' is not an alphabet.", a);
    }

    return 0;
}
